{
  "name": "ayzd-api",
  "version": "1.0.0",
  "homepage": "./",
  "description": "ayzd admin application",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "admin": "npm start --prefix client_admin",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install webpack@4.44.2 --prefix && npm install --prefix client && npm run build --prefix client"
  },
  "author": "author",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.983.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.10",
    "concurrently": "^5.0.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.2",
    "multer": "^1.4.3",
    "multer-s3": "^2.9.0",
    "node-schedule": "^2.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "rss-parser": "^3.12.0"
  },
  "engines": {
    "node": "14.17.0"
  }
}
